net: "task/scrape_o/train_val.prototxt"
test_iter: 4
test_interval: 10
test_compute_loss: 1
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.00001
lr_policy: "fixed"
# gamma: 0.1
# stepsize should also be lower, as we're closer to being done
# stepsize: 1000
display: 1
max_iter: 5000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "task/scrape/conv2_blue/"
solver_mode: GPU
