net: "task/inadcl/train_val.prototxt"
test_iter: 1
test_interval: 5
test_compute_loss: 1
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 10000
display: 1
max_iter: 30000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "task/inadcl/"
solver_mode: GPU
